{
  "priority": 1,
  "request" : {
    "urlPattern" : "/ws/9([0-9]{7})/json/",
    "method" : "GET"
  },
  "response" : {
    "status" : 404,
	  "bodyFileName" : "rest_v2_cep_invalid_body.json",
    "headers" : {
      "Content-Type" : "application/json;charset=utf-8"
    },
    "fixedDelayMilliseconds": 2000,
    "transformers": ["response-template"]
  }
}